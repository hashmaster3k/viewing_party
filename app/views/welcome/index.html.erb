<center>
<h1>Welcome to Viewing Party</h1>
<h4>An app to watch movies with your friends</h4>

<div id="login">
  <%= form_with url: login_path, local: true do |form| %>
    <%= form.text_field :email, placeholder: "Email" %><br/><br>
    <%= form.password_field :password, placeholder: "Password" %><br/><br>
    <%= form.submit "Sign In" %><br/><br>
  <% end %>
</div>

<%= link_to "New to Viewing Party? Register Here", registration_path %>.
</center>
