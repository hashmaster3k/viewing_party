<center>
<h1>Welcome to Viewing Party</h1>
<h4>An app to watch movies with your friends</h4>

<div id="login">
  <%= form_with url: "#", local: true do |form| %>
    <%= form.text_field :email, value: "Email" %><br/><br>
    <%= form.password_field :password, value: "Password" %><br/><br>
    <%= form.submit "Sign In" %><br/><br>
  <% end %>
</div>

<%= link_to "New to Viewing Party? Register Here", "#" %>

</center>